  number = {C7},
  urldate = {2016-10-26},
  journal = {Journal of Geophysical Research: Oceans},
  author = {Niebauer, H. J. and Royer, Thomas C. and Weingartner, Thomas J.},
  month = jul,
  year = {1994},
  keywords = {4219 Continental shelf and slope processes,4223 Descriptive and regional oceanography,4243 Marginal and semi-enclosed seas},
  pages = {14113--14126},
  file = {Snapshot:/Users/eric.ward/Library/Application Support/Zotero/Profiles/6ic0vo4n.default/zotero/storage/8RXCPMVX/abstract.html:text/html},
  groups = {pfx-pws}
