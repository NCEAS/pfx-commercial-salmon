  month = jan,
  year = {2008},
  keywords = {age-structured assessment,hydroacoustic survey,Pacific herring,Prince William Sound},
  pages = {25--43},
  file = {Full Text PDF:/Users/eric.ward/Library/Application Support/Zotero/Profiles/6ic0vo4n.default/zotero/storage/75QHKCAE/Hulson et al. - 2008 - Data conflicts in fishery models incorporating hy.pdf:application/pdf;Snapshot:/Users/eric.ward/Library/Application Support/Zotero/Profiles/6ic0vo4n.default/zotero/storage/K6WJ77XW/25.html:text/html},
