  month = jan,
  year = {2009},
  keywords = {environmental change,North Sea herring,recruitment,regime shift,sustainability},
  pages = {272--277},
  file = {Full Text PDF:/Users/eric.ward/Library/Application Support/Zotero/Profiles/6ic0vo4n.default/zotero/storage/FZW37ZF8/Payne et al. - 2009 - Recruitment in a changing environment the 2000s N.pdf:application/pdf;Snapshot:/Users/eric.ward/Library/Application Support/Zotero/Profiles/6ic0vo4n.default/zotero/storage/2UTT3ZTV/272.html:text/html},
