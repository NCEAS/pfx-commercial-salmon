  author = {Williams, Erik H. and Quinn, Terrance J., II},
  month = dec,
  year = {2000},
  keywords = {Bering Sea,multivariate cluster analysis,north-east Pacific Ocean,Pacific herring,recruitment,weight-at-age,year-class strength},
  pages = {285--299},
