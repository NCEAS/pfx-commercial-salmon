  number = {2},
  urldate = {2016-10-26},
  journal = {Marine Mammal Science},
  author = {Frost, Kathryn J. and Lowry, Lloyd F. and Ver Hoef, Jay M.},
  month = apr,
  year = {1999},
  keywords = {aerial surveys,Exxon Valdez oil spill,generalized linear model,harbor seal,Phoca vitulina richardsi,Poisson regression,population monitoring,Prince William Sound,trend analysis},
  pages = {494--506},
  file = {Snapshot:/Users/eric.ward/Library/Application Support/Zotero/Profiles/6ic0vo4n.default/zotero/storage/NEZKQ26E/abstract.html:text/html},
