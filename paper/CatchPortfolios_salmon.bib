  month = jun,
  year = {2016},
  keywords = {Fisheries Management,management procedure,Management strategy evaluation,simulation,stakeholders,uncertainty},
  pages = {303--334},
  file = {Full Text PDF:/Users/eric.ward/Library/Application Support/Zotero/Profiles/6ic0vo4n.default/zotero/storage/64X2ISA8/Punt et al. - 2016 - Management strategy evaluation best practices.pdf:application/pdf;Snapshot:/Users/eric.ward/Library/Application Support/Zotero/Profiles/6ic0vo4n.default/zotero/storage/GIBR7FZN/abstract.html:text/html},
